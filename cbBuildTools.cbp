<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbBuildTools" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="to_codeblocks_wx28">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/to_codeblocks_wx28" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-dde --no-check-associations " />
				<Option host_application="$(#cb)/devel/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)/gcc_dll/mswu" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -9  $(#cb)\devel\share\codeblocks\cbBuildTools.zip images\*" />
					<Add after="zip -j9 $(#cb)\devel\share\codeblocks\cbBuildTools.zip res\*" />
					<Add after="zip -j9 $(#cb)\devel\share\codeblocks\cbBuildTools.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="to_codeblocks_wx30">
				<Option output="$(#cb)/devel30/share/codeblocks/plugins/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/to_codeblocks_wx30" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-dde --no-check-associations " />
				<Option host_application="$(#cb)/devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx30.include)" />
					<Add directory="$(#wx30.lib)/gcc_dll/mswu" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add library="wxmsw30u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx30.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -9  $(#cb)\devel30\share\codeblocks\cbBuildTools.zip images\*" />
					<Add after="zip -j9 $(#cb)\devel30\share\codeblocks\cbBuildTools.zip res\*" />
					<Add after="zip -j9 $(#cb)\devel30\share\codeblocks\cbBuildTools.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="create_plugin_wx28">
				<Option output="output/wx28/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/plugin_wx28" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="$(#cb)/devel28/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)/gcc_dll/mswu" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if not exist plugin_wx28 mkdir plugin_wx28" />
					<Add after="zip -j9 plugin_wx28\cbBuildTools.zip manifest.xml" />
					<Add after="zip -9  plugin_wx28\cbBuildTools.zip images\*" />
					<Add after="zip -j9 plugin_wx28\cbBuildTools.zip res\*" />
					<Add after="zip -j9 plugin_wx28\cbBuildTools.cbplugin $(TARGET_OUTPUT_FILE) plugin_wx28\cbBuildTools.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="create_plugin_wx30">
				<Option output="output/wx30/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/plugin_wx30" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="$(#cb)/devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx30.include)" />
					<Add directory="$(#wx30.lib)/gcc_dll/mswu" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add library="wxmsw30u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx30.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if not exist plugin_wx30 mkdir plugin_wx30" />
					<Add after="zip -j9 plugin_wx30\cbBuildTools.zip manifest.xml" />
					<Add after="zip -9  plugin_wx30\cbBuildTools.zip images\*" />
					<Add after="zip -j9 plugin_wx30\cbBuildTools.zip res\*" />
					<Add after="zip -j9 plugin_wx30\cbBuildTools.cbplugin $(TARGET_OUTPUT_FILE) plugin_wx30\cbBuildTools.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="to_codeblocks_wx28;to_codeblocks_wx30;create_plugin_wx28;create_plugin_wx30;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-std=gnu++11" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/sdk/wxscintilla/include" />
			<Add directory="$(#cb)/include/tinyxml" />
			<Add directory="include" />
		</Compiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<Unit filename="cbBuildTools.cpp" />
		<Unit filename="cbBuildTools.h" />
		<Unit filename="include/buildtargetswindow.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="src/buildtargetswindow.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
